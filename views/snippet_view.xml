<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="external_snippets" inherit_id="website.snippets"
              priority="20">
        <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]"
               position="after">
            <t t-snippet="web_recently_viewed_product.most_viewed_snippet"/>
        </xpath>
    </template>
</odoo>